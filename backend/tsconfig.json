{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
